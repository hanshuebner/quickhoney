      <a onclick="show_directory_buttons('vector')" href="#vector" id="m_vector">
        <img alt="" class="unselected" src="/image/vector-unselected" />
        <img alt="" class="selected" src="/image/vector-selected" />
      </a>
      <a onclick="show_directory_buttons('pen')" href="#pen" id="m_pen">
        <img alt="" class="unselected" src="/image/pen-unselected" />
        <img alt="" class="selected" src="/image/pen-selected" />
      </a>
      <a href="#news" id="m_news">
        <img alt="" class="unselected" src="/image/news-unselected" />
        <img alt="" class="selected" src="/image/news-selected" />
      </a>
      <a href="#contact" id="m_contact">
        <img alt="" class="unselected" src="/image/contact-unselected" />
        <img alt="" class="selected" src="/image/contact-selected" />
      </a>
    </div>

    <div id="loading" class="page">
      <quickhoney:simple-image-browser>
        <img alt="" src="/image/startscreen" id="logo" border="0" width="438" height="67" />

