  <body id="body" class="loading yui-skin-sam">

    <div id="site">
      <div id="content">
        <div id="overlay">
        </div>

        <div id="menu">
          <div id="menu-backdrop">
            <a onclick="show_home_buttons()" href="#home" id="m_home">
              <img alt="" class="menu" src="/static/images/logo-c.png" />
            </a>
            <a onclick="show_directory_buttons('pixel')" href="#pixel" id="m_pixel">
