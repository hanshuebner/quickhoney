        <p>Works: <a href="/vector">vector</a> and <a href="/pen">pen</a></p>
        <h1>Nana Rausch</h1>
        <p>Phone 646.270.5592<br/>Email: n@quickhoney.com</p>
        <p>Works: <a href="/pixel">pixel</a></p>
      </quickhoney:simple-image-browser>
    </div>

    <div id="home" class="page">
    </div>

    <div id="image_browser" class="page">

      <div id="directory" class="subform">
      </div>
      
      <div id="results" class="subform">
        <div id="thumbnails">
        </div>
      </div>
      
      <div id="browse" class="subform">
        <div class="navbar">
          <div class="left">
            <span id="image_navbar" class="text"> </span>
            <span id="full_click" class="text"> </span>
          </div>
          <div class="right">
            <span id="back_to_results"><a id="back_to_results_link" href="#">back to results</a></span>
            <span id="result_image_count"> </span>
          </div>
        </div>
        
        <div id="image_action_buttons" class="buttonbox">
        </div>

        <div id="image_detail">
        </div>
        
        <div id="metadata" class="text">
          (not available)
        </div>
      </div>

    </div>
    
    <div id="news" class="page">
      <table border="0">
        <tbody>
          <tr>
            <td valign="top" id="newsentries">
            </td>
            <td class="archive" id="archive-navigation" valign="top" align="right">
            </td>
          </tr>
        </tbody>
      </table>
    </div>

    <div id="contact" class="page">
      <div style="height: 300px">
        <img id="contactimage" alt="" src="/image/pixelcontact"/>
        <br /><br />
